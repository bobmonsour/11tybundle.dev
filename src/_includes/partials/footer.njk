<footer data-pagefind-ignore>
  {% include 'partials/bundleresources.njk' %}
{#
  Note: the header and footer include different navigation.
#}
  <nav>
    <ul class="brand-nav">
      {% for link in site.footerNavLinks %}
        <li>
          <a href="{{ link.url }}" {% if link.rel %}rel="{{ link.rel }}" title="{{link.title}}" type="{{link.type}}" {% endif %}class="nav-link" {{ link.text | isCurrentPage(page.url) | safe }}>{{ link.text }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
</footer>
